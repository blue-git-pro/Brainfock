{
  "name": "Member",
  "_base": "User",
  "base": "PersistedModel",
  "idInjection": false,
  "mysql": {
    "schema": "brainfock_node",
    "table": "user"
  },
  "options": {
    "validateUpsert": true
  },
  "relations":{
    "roleMappings": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId",
      "scope": {
        "where": {
          "principalType": "USER"
        }
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "properties": {
    "realm": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "credentials": {
      "type": "object",
      "deprecated": true
    },
    "challenges": {
      "type": "object",
      "deprecated": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "emailVerified": "boolean",
    "verificationToken": "string",
    "status": "string",
    "created": "date",
    "lastUpdated": "date"
  },
  "hidden": ["password"],
  "acls": []
}
